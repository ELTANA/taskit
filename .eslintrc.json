{
  "env": {
    "browser": true,
    "es2022": true,
    "commonjs": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      ".eslintrc": true,
      "typescript": false
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "jsx-quotes": "off",
    "no-unused-vars": "off",
    "no-console": "warn",
    "func-names": "off",
    "object-shorthand": "off"
  }
}
